@charset "utf-8";
/* CSS Document */
.main_div                                    {width:1200px;min-height:400px;border:1px solid #FFF;background:transparent;background-color:rgba(255,255,255,0.75);border-radius:8px;margin-top:40px;}
.pay_table td                                {width:300px;text-align:center;font-size:20px;}
.pay_money                                   {color:#ED1C24;}
.user_name                                   {font-size:30px;text-align:center;color:#3DEBED;}
.addr_city                                   {text-align:left;color:#930;margin-top:30px;}
.addr_street                                 {width:200px;height:40px;text-align:left;color:#930}
.click_a                                     {height:60px;}
.click_a a                                   {text-decoration:none;width:90px;height:40px;display:block;color:#333;background:#BBEDFA;line-height:40px;border-radius:10px;}
.click_a a:hover                             {background:#DB9FFD}
