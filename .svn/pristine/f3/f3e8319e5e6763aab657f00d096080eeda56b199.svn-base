@charset "utf-8";
/* CSS Document */
*                                             {margin:0;padding:0;}
body                                     {background-color: #ccffff;}
.bg_pic                                  {width:100%;height:840px;position:relative;background:url(../IMAGES/regpage_bg0807.jpg) -300px 0px no-repeat;background-size:1920px 840px;position:absolute;left:0px;top:0px;z-index:-999;border:1px solid black;}
#bg_pic_2                                {position:absolute;top:80px;left:20%;}
#bg_pic_3                                {margin-left: 20%;}
table                                         {margin-left: 36%;}
td                                       {height:40px;}
.msg                                     {display:block;width:120px;height:40px;line-height:40px;text-align:left;}
.form_data                               {width:160px;height:25px;font-size:18px;color:#0FF;border:1px solid #0FC;border-radius:6px;}
#register                                {display:block;width:60px;height:30px;background:url(../IMAGES/button_2.png) no-repeat;text-align:center;line-height:30px;text-decoration:none;color:#0CC;}
